{
    "version": 2,
    "buildCommand": "npm run build",
    "functions": {
      "api/index.ts": {
        "runtime": "@vercel/node@3"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.ts"
      }
    ]
  }
  